{
  "name": "electron-quick-start",
  "version": "0.0.2",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^22.3.2"
  },

  "dependencies": {
    "electron-simple-updater": "^2.0.6",
    "yarn": "^1.22.0"
  },

  "electronBuilder": {
    "build": {
      "productName": "Node App",
      "appId": "my.id"
    },
    "directories": {
      "output": "releases",
      "app":"releases"
    }
  }

}
